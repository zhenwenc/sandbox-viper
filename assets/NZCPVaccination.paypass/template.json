{
  "templateId": "nzcp",
  "description": "COVID Vaccination Card",
  "objectType": "covidCardObject",
  "objectTemplate": {
    "id": "{{ meta.id }}",
    "issuerId": "{{ meta.issuerId }}",
    "title": "NZ COVID Pass",
    "cardColorHex": "#E6E5F0",
    "expiration": "{{ data.exp }}",
    "validFrom": "2021-10-01",
    "heroImage": {
      "sourceUri": {
        "description": "Ministry of Health Manatū Hauora",
        "uri": "https://www.datocms-assets.com/38428/1634014220-strip-android.png"
      }
    },
    "logo": {
      "sourceUri": {
        "description": "Ministry of Health Manatū Hauora",
        "uri": "https://www.datocms-assets.com/38428/1634013837-moh-840-full.png"
      }
    },
    "barcode": {
      "type": "qrCode",
      "value": "{{ meta.barcode }}",
      "alternateText": "{{ data.jti }}"
    },
    "patientDetails": {
      "dateOfBirth": "{{ data.ext.dob }}",
      "patientName": "{{ data.ext.name }}"
    }
  }
}
