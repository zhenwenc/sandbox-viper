{
  "templateId": "nzcp",
  "description": "NZ COVID Pass",
  "objectType": "covidCardObject",
  "objectTemplate": {
    "id": "{{ meta.id }}",
    "issuerId": "{{ meta.issuerId }}",
    "title": "NZ COVID Pass",
    "cardColorHex": "#E6E5F0",
    "expiration": "{{ data.ext.exp }}",
    "validFrom": "{{ data.ext.iat }}",
    "dateFormat": "DATE_YEAR",
    "logo": {
      "sourceUri": {
        "description": "Ministry of Health ManatÅ« Hauora",
        "uri": "https://www.datocms-assets.com/38428/1634013837-moh-840-full.png"
      }
    },
    "heroImage": {
      "sourceUri": {
        "description": "MoH",
        "uri": "https://www.datocms-assets.com/38428/1634014220-strip-android.png"
      }
    },
    "barcode": {
      "type": "qrCode",
      "value": "{{ meta.barcode }}",
      "alternateText": "{{ data.jti }}"
    },
    "patientDetails": {
      "dateOfBirth": "{{ data.ext.dob }}",
      "patientName": "{{ data.ext.name }}"
    },
    "textModulesData": [
      {
        "header": "Disclaimer",
