{
  "templateId": "loyalty",
  "description": "Loyalty Pass",
  "classType": "loyaltyClass",
  "classTemplate": {
    "version": "1",
    "id": "__PLACEHOLDER__",
    "reviewStatus": "underReview",
    "title": "Demo Loyalty Pass",
    "programLogo": {
      "kind": "walletobjects#image",
      "sourceUri": {
        "uri": "https://gpay-loyaltyapi-demo.web.app/images/logo.png"
      }
    },
    "localizedProgramName": {
      "kind": "walletobjects#localizedString",
      "defaultValue": {
        "kind": "walletobjects#translatedString",
        "language": "en-GB",
        "value": "Demo Loyalty Card"
      }
    },
    "allowMultipleUsersPerObject": true,
    "countryCode": "NZ",
    "enableSmartTap": false,
    "localizedIssuerName": {
      "kind": "walletobjects#localizedString",
      "defaultValue": {
        "kind": "walletobjects#translatedString",
        "language": "en-GB",
        "value": "Codelab Demo"
      }
    },
    "multipleDevicesAndHoldersAllowedStatus": "multipleHolders"
  },
  "objectType": "loyaltyObject",
  "objectTemplate": {
    "id": "{{ meta.id }}",
    "classId": "{{ meta.classId }}",
    "barcode": {
      "type": "qrCode",
      "value": "{{ meta.barcode }}",
      "alternateText": "Loyalty Point Redeem Code"
    },
    "accountId": "{{ data.accountId }}",
    "accountName": "{{ data.accountName }}",
    "state": "active",
    "loyaltyPoints": {
      "balance": {
        "int": 101
      },
      "label": "Points"
    }
  }
}
